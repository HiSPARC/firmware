                 "      end if;"
                 "    end if;"
                 "  end process;"
                 ""
                 "  process (RDCLOCK)"
                 "  BEGIN"
                 "    if (RDCLOCK'event and RDCLOCK = '1') then"
                 "      DATA_OUT <= RAM_BLOCK(RD_ADDRESS_REG);"
                 "      RD_ADDRESS_REG <= RD_ADDRESS;"
                 "    end if;"
                 "  end process;"
                 ""
                 "end architecture a0 ; -- of EVENT_FIFO"
